{
  "name": "@nomercy-entertainment/nomercy-music-player",
  "version": "0.1.0",
  "description": "Full event-driven music player without a UI.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "lint": "eslint --fix .",
    "test": "jest",
    "prepublishOnly": "npm run build"
  },
  "author": "NoMercy Entertainment",
  "repository": {
    "type": "git",
    "url": "https://github.com/NoMercy-Entertainment/NoMercyMusicPlayer.git"
  },
  "license": "MIT",
  "dependencies": {
    "@ionic/vue": "^8.4.1",
    "@nomercy-entertainment/media-session": "^0.1.8",
    "audiomotion-analyzer": "^4.5.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "typescript": "5.3.3"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "src",
    "./*"
  ],
  "exports": {
    "./index": {
      "import": "./src/index.ts",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./audiomotionAnalyzer": {
      "import": "./src/audiomotionAnalyzer.ts",
      "require": "./dist/audiomotionAnalyzer.js",
      "types": "./dist/audiomotionAnalyzer.d.ts"
    },
    "./src/audioNode": {
      "import": "./src/audioNode.ts",
      "require": "./dist/audioNode.ts",
      "types": "./dist/audioNode.d.ts"
    },
    "./src/equalizer": {
      "import": "./src/equalizer.ts",
      "require": "./dist/equalizer.ts",
      "types": "./dist/equalizer.d.ts"
    },
    "./src/helpers": {
      "import": "./src/helpers.ts",
      "require": "./dist/helpers.ts",
      "types": "./dist/helpers.d.ts"
    },
    "./src/queue": {
      "import": "./src/queue.ts",
      "require": "./dist/queue.ts",
      "types": "./dist/queue.d.ts"
    },
    "./src/state": {
      "import": "./src/state.ts",
      "require": "./dist/state.ts",
      "types": "./dist/state.d.ts"
    },
    "./src/types": {
      "types": "./dist/types.d.ts"
    }
  }
}
